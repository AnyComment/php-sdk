{
    "name": "anycomment/php-sdk",
    "description": "PHP SDK to communicate with AnyComment REST API",
    "keywords": ["anycomment", "anycomment rest api"],
    "type": "library",
    "authors": [
        {
            "name": "Alexander Teshabaev",
            "email": "sasha.tesh@gmail.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "AnyComment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnyComment\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "6.5.5",
        "netresearch/jsonmapper": "3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
