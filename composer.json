{
    "name": "anycomment/php-sdk",
    "description": "PHP SDK to communicate with AnyComment REST API",
    "keywords": ["anycomment", "anycomment rest api"],
    "type": "library",
    "authors": [
        {
            "name": "Alexander Teshabaev",
            "email": "sasha.tesh@gmail.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "AnyComment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnyComment\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6|^7.0",
        "guzzlehttp/guzzle": "6.5.5",
        "netresearch/jsonmapper": "3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "paragonie/random_compat": "Provides a better CSPRNG option in PHP 5",
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
